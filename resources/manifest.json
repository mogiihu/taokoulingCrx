{
  "name": "解析淘口令的软件",
  "version": "2.1.1",
  "description": "一个会自动解析淘口令的软件",
  "permissions": [
    "https://api.taokouling.com/*",
    "http://tbk.moonkop.com/*",
    "webRequest",
    "clipboardRead"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "解析淘口令的软件",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
